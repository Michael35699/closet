{
  "name": "closetjs",
  "author": {
    "name": "Henrik Giesel",
    "email": "hengiesel@gmail.com",
    "url": "https://github.com/hgiesel"
  },
  "license": "GPL-3.0",
  "version": "0.1.4",
  "browser": "index.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/hgiesel/closet.git"
  },
  "scripts": {
    "compile-parser": "bin/compile-parser.sh",
    "create-favicon": "bin/create-favicon.sh",
    "install-dependencies": "bin/install-dependencies.sh",
    "b": "webpack --config webpack.config.js",
    "build": "run-p build:docs build:main build:anki",
    "build:main": "run-s compile-parser pack:main",
    "build:docs": "run-s compile-parser create-favicon pack:docs",
    "build:anki": "run-s compile-parser pack:anki",
    "setup-test": "bin/setup-test.sh",
    "pack:main": "webpack -p",
    "pack:docs": "webpack -p --env.destination=docs",
    "pack:anki": "webpack -p --env.destination=anki",
    "watch": "run-p watch-docs watch-main watch-anki",
    "watch:main": "run-s compile-parser pack:watch:main",
    "watch:docs": "run-s compile-parser pack:watch:docs",
    "watch:anki": "run-s compile-parser pack:watch:anki",
    "pack:watch:main": "webpack -d",
    "pack:watch:docs": "webpack -d --env.destination=docs",
    "pack:watch:anki": "webpack -d --env.destination=anki",
    "serve:docs": "bin/serve.sh",
    "serve:test": "serve -l 59045",
    "link": "bin/link.sh -c",
    "dev:docs": "run-p serve-docs watch:docs",
    "dev:anki": "run-s watch:anki",
    "zip": "run-s build:anki zip:sub",
    "zip:sub": "bin/zip.sh",
    "test": "jest",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "moo": "^0.5.1",
    "nearley": "^2.19.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-strip": "^1.3.3",
    "@rollup/plugin-typescript": "^3.1.1",
    "@types/moo": "^0.5.3",
    "@types/nearley": "^2.11.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "eslint": "^7.7.0",
    "jest": "^26.4.2",
    "mocha": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.32.1",
    "rollup-plugin-terser": "^5.3.0",
    "serve": "^11.3.2",
    "terser-webpack-plugin": "^4.1.0",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7",
    "webpack": "^5.0.0-beta.29",
    "webpack-cli": "^3.3.12"
  }
}
